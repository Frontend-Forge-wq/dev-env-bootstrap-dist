{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        { "type": "path", "style": "plain", "foreground": "#E6E6E6", "properties": { "style": "folder" } },
        { "type": "git", "style": "plain", "foreground": "#DCE775", "properties": { "fetch_status": true, "display_status": true, "display_stash_count": true } },
        { "type": "executiontime", "style": "plain", "foreground": "#F9E2AF", "properties": { "threshold": 120 } }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        { "type": "time", "style": "plain", "foreground": "#A0A0A0", "properties": { "time_format": "15:04:05" } }
      ]
    }
  ]
}